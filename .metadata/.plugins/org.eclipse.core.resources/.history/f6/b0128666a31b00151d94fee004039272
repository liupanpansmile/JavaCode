package com.oneapm.staticFinalTest;

public class Test2 {
	
	
	private static int  staticVar = 0 ;
	

	static
	{
		staticVar = 1 ;
		System.out.println("staticVar = " + staticVar);
		
		Test2 t = new  Test2() ; 
		t.func() ;
	}
	
	public Test2()
	{
		System.out.println("Construction Called !");
	}
	
	public void func()
	{
		System.out.println("func called !");
	}
}
